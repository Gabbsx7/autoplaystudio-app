{
  "name": "autoplaystudio",
  "version": "0.1.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "dev": "pnpm --filter @autoplaystudio/web dev",
    "build": "pnpm --filter @autoplaystudio/web build",
    "start": "pnpm --filter @autoplaystudio/web start",
    "lint": "pnpm --recursive lint",
    "type-check": "pnpm --recursive type-check",
    "clean": "pnpm --recursive clean"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "typescript": "^5.3.2",
    "supabase": "^1.123.4"
  }
}
